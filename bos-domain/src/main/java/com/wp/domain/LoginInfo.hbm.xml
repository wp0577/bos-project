<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wp.domain.LoginInfo" table="loginInfo" >
        <id name="id" type="java.lang.String">
            <column name="id" length="32" />
            <generator class="uuid" />
        </id>
        <property name="ipString" type="java.lang.String">
            <column name="ipString" length="20" />
        </property>
        <property name="lastTimeString" type="java.util.Date">
            <column name="lastTimeString" length="0" />
        </property>
        <property name="firstTimeString" type="java.util.Date">
            <column name="firstTimeString" length="0"></column>
        </property>
        <property name="totalCount" type="java.lang.Integer">
            <column name="totalCount" />
        </property>

    </class>
</hibernate-mapping>
